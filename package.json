    "jsdom": "15.2.1",
    "mocha": "7.0.1",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "precss": "4.0.0",
    "prop-types": "15.7.2",
    "raf": "3.4.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hot-loader": "4.12.19",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-svg-loader": "2.1.0",
    "rimraf": "3.0.1",
    "sass-loader": "8.0.2",
    "sinon": "8.1.1",
    "style-loader": "1.1.3",
    "styled-components": "5.0.0",
